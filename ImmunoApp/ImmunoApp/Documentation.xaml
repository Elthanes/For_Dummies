<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ImmunoApp.Documentation"
             Title="Übersicht"
             Icon="ic_history">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="neue Einahme"/>
            <Button Text="Historie"/>
            <Label Text="Wie fühlen Sie sich heute?" VerticalOptions="Center" HorizontalOptions="CenterAndExpand" />
            <Editor MinimumHeightRequest="50" VerticalOptions = "FillAndExpand" BackgroundColor="Blue" Margin="10,10,10,10"/>
            <Button Text="Speichern"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>